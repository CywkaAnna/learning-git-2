  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.24.5",
    "curl": "^0.1.4",
    "globstar": "^1.0.0",
    "mkdirp": "^0.5.1",
    "nu-html-checker": "^0.1.0",
    "touch": "^3.1.0"
  }
